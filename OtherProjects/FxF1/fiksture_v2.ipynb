    "from datetime import datetime, timedelta\n",
    "\n",
    "# Tarihler arasında veri üretmek için bir fonksiyon\n",
    "def tarih_araliginda_veri_uret(baslangic_tarih_str, bitis_tarih_str):\n",
    "    # Verilen tarih formatına göre tarihleri dönüştürelim\n",
    "    baslangic_tarih = datetime.strptime(baslangic_tarih_str, \"%Y-%m-%d\")\n",
    "    bitis_tarih = datetime.strptime(bitis_tarih_str, \"%Y-%m-%d\")\n",
    "    \n",
    "    # Tarihler arasında gün sayısı kadar veri üretelim\n",
    "    tarih_listesi = []\n",
    "    while baslangic_tarih <= bitis_tarih:\n",
    "        tarih_listesi.append(baslangic_tarih.strftime(\"%Y-%m-%d\"))\n",
    "        baslangic_tarih += timedelta(days=1)\n",
    "    \n",
    "    return tarih_listesi\n",
    "\n",
    "baslangic_tarih = \"2024-08-01\"\n",
    "bitis_tarih = \"2024-10-07\"\n",
    "\n",
    "tarih_listesi = tarih_araliginda_veri_uret(baslangic_tarih, bitis_tarih)\n",
    "for tarih in tarih_listesi:\n",
    "    try:\n",
    "        api = ResultAPI(date = tarih)\n",
    "        data  = api.AddDataBase()\n",
    "        print(\"Alınan Tarih\",tarih)\n",
    "    except:\n",
    "        print(\"Bu tarihte maç yok\",tarih)\n",
    "print(\"İşlemler Bitti\")    "
   ]
